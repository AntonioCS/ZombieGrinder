<xml>
	<settings>
		<name>tripmine_fire</name>
	</settings>
	<emitters>
		<emitter>
			<settings>
				<important>true</important>
				<name>Projectile</name>
				<render_pass>geometry</render_pass>
				<lock_rotation_to_direction>true</lock_rotation_to_direction>
				<keep_effect_alive>true</keep_effect_alive>
				<global_max_particles>0</global_max_particles>
				<draw_method>Beam</draw_method>
				<blend_mode>Alpha</blend_mode>
				<cycle_length>5</cycle_length>
				<cycle_count>1</cycle_count>
				<max_particles_per_cycle>1</max_particles_per_cycle>
				<allow_bouncing>true</allow_bouncing>
				<beam_use_seperate_tail_head>false</beam_use_seperate_tail_head>
				<sprite>particle_tripemine_beam_main</sprite>
				<affected_by_attractors>false</affected_by_attractors>
				<move_based_on_direction>true</move_based_on_direction>
				<enable_collision>true</enable_collision>
				<collision_reponse>Stop</collision_reponse>
				<spawn_shape>Point</spawn_shape>
				<collision_scale_x>0.25</collision_scale_x>
				<collision_scale_y>0.25</collision_scale_y>
				<collision_offset_x>-20</collision_offset_x>
				<collision_offset_y>-20</collision_offset_y>
				<spawn_inner_radius>0</spawn_inner_radius>
				<spawn_outer_radius>0</spawn_outer_radius>
				<track_effect>true</track_effect>
				<draw_shadow>false</draw_shadow>
				<offset_by_height>false</offset_by_height>
				<spawn_offset_x>0</spawn_offset_x>
				<spawn_offset_y>0</spawn_offset_y>
				<scale_proportional>false</scale_proportional>
				<invert_scale_on_bounce>false</invert_scale_on_bounce>
				<add_emitter_direction>true</add_emitter_direction>
				<layer_offset>0</layer_offset>
				<depth_bias>10</depth_bias>
				<penetration_speed_scalar>1</penetration_speed_scalar>
				<enable_damage>true</enable_damage>
				<constant_damage>true</constant_damage>
				<damage_offset_x>-20</damage_offset_x>
				<damage_offset_y>-20</damage_offset_y>
				<damage_radius>3</damage_radius>
				<damage_type>Projectile</damage_type>
				<collection_enabled>false</collection_enabled>
				<collection_value>0</collection_value>
				<collection_type>Coin</collection_type>
				<is_projectile>true</is_projectile>
			</settings>
			<curves>
				<Sprite_Frame start_min="0" start_max="0" end_min="60" end_max="60">
					<point x="0" y="0.989726"/>
					<point x="1" y="0"/>
				</Sprite_Frame>
				<Direction start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Direction>
				<Spawn_Rate start_min="1000" start_max="1000" end_min="1000" end_max="1000">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Spawn_Rate>
				<Velocity_X start_min="-200" start_max="-200" end_min="-200" end_max="-200">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_X>
				<Velocity_Y start_min="-200" start_max="-200" end_min="-200" end_max="-200">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Y>
				<Velocity_Height start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Height>
				<Velocity_Rotation start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Rotation>
				<Velocity_Direction start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Direction>
				<Color_R start_min="1" start_max="1" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Color_R>
				<Color_G start_min="1" start_max="1" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Color_G>
				<Color_B start_min="1" start_max="1" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Color_B>
				<Color_A start_min="0" start_max="0" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Color_A>
				<Scale_X start_min="0" start_max="0" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="0.861021" y="0.982906"/>
					<point x="0.967427" y="0.529915"/>
					<point x="1" y="0.534188"/>
				</Scale_X>
				<Scale_Y start_min="0" start_max="0" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="1"/>
				</Scale_Y>
				<Lifetime start_min="0" start_max="0" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Lifetime>
				<Rotation start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="0.971429"/>
					<point x="1" y="1"/>
				</Rotation>
				<Height start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Height>
				<Damage start_min="0.001" start_max="0.001" end_min="0.001" end_max="0.001">
					<point x="0" y="1"/>
					<point x="1" y="1"/>
				</Damage>
			</curves>
			<events>
				<event x="0" probability="1" type="Script Event" trigger="Penetration" param="On_Beam_Trigger"/>
			</events>
		</emitter>
	</emitters>
	<attractors/>
</xml>

