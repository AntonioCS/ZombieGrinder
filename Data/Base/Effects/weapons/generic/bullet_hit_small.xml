<xml>
  <editor>
    <asset_type>ParticleFX</asset_type>
  </editor>
	<settings>
		<name>bullet_hit_small</name>
	</settings>
	<emitters>
		<emitter> 
			<settings>
				<name>Hit</name>
				<important>false</important>
				<render_pass>geometry</render_pass>
				<lock_rotation_to_direction>true</lock_rotation_to_direction>
				<keep_effect_alive>true</keep_effect_alive>
				<global_max_particles>0</global_max_particles>
				<draw_method>Sprite</draw_method>
				<cycle_length>1</cycle_length>
				<cycle_count>1</cycle_count>
				<max_particles_per_cycle>1</max_particles_per_cycle>
				<sprite>particle_bullet_hit_0</sprite>
				<affected_by_attractors>false</affected_by_attractors>
				<move_based_on_direction>true</move_based_on_direction>
				<enable_collision>false</enable_collision>
				<collision_reponse>Stop</collision_reponse>
				<spawn_shape>Point</spawn_shape>
				<spawn_inner_radius>10</spawn_inner_radius>
				<spawn_outer_radius>10</spawn_outer_radius>
				<track_effect>false</track_effect>
				<draw_shadow>false</draw_shadow>
				<offset_by_height>false</offset_by_height>
				<spawn_offset_x>0</spawn_offset_x>
				<spawn_offset_y>0</spawn_offset_y>
				<scale_proportional>false</scale_proportional>
				<invert_scale_on_bounce>false</invert_scale_on_bounce>
				<add_emitter_direction>true</add_emitter_direction>
				<layer_offset>0</layer_offset>
				<depth_bias>0</depth_bias>
			</settings>
			<curves>
				<Sprite_Frame start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Sprite_Frame>
				<Direction start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Direction>
				<Spawn_Rate start_min="100" start_max="100" end_min="1000" end_max="1000">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Spawn_Rate>
				<Velocity_X start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_X>
				<Velocity_Y start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Y>
				<Velocity_Height start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Height>
				<Velocity_Rotation start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Rotation>
				<Velocity_Direction start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Direction>
				<Color_R start_min="1" start_max="1" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Color_R>
				<Color_G start_min="1" start_max="1" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Color_G>
				<Color_B start_min="1" start_max="1" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Color_B>
				<Color_A start_min="0" start_max="0" end_min="1" end_max="1">
					<point x="0" y="0.990099"/>
					<point x="0.044711" y="0.966997"/>
					<point x="0.046892" y="0.924092"/>
					<point x="0.5747" y="0.669967"/>
					<point x="1" y="0"/>
				</Color_A>
				<Scale_X start_min="0" start_max="0" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Scale_X>
				<Scale_Y start_min="0" start_max="0" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Scale_Y>
				<Lifetime start_min="0" start_max="0" end_min="0.15" end_max="0.15">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Lifetime>
				<Rotation start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Rotation>
				<Height start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Height>
			</curves>
			<events/>
		</emitter>
	</emitters>
	<attractors/>
</xml>

