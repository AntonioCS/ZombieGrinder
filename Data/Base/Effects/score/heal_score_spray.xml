				<Rotation start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Rotation>
				<Height start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Height>
				<Damage start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Damage>
			</curves>
			<events/>
