<xml>
	<settings>
		<name>chaser_zombie_blood_splat</name>
	</settings>
	<emitters>
		<emitter>
			<settings>
				<name>Blood For The Blood God!</name>
				<important>false</important>
				<render_pass>geometry</render_pass>
				<lock_rotation_to_direction>true</lock_rotation_to_direction>
				<keep_effect_alive>false</keep_effect_alive>
				<global_max_particles>0</global_max_particles>
				<draw_method>Sprite</draw_method>
				<cycle_length>0.01</cycle_length>
				<cycle_count>1</cycle_count>
				<max_particles_per_cycle>1</max_particles_per_cycle>
				<sprite>particle_blood_main</sprite>
				<affected_by_attractors>false</affected_by_attractors>
				<move_based_on_direction>true</move_based_on_direction>
				<enable_collision>false</enable_collision>
				<collision_reponse>Bounce</collision_reponse>
				<spawn_shape>Circle</spawn_shape>
				<collision_scale_x>1</collision_scale_x>
				<collision_scale_y>1</collision_scale_y>
				<collision_offset_x>0</collision_offset_x>
				<collision_offset_y>0</collision_offset_y>
				<spawn_inner_radius>0</spawn_inner_radius>
				<spawn_outer_radius>0</spawn_outer_radius>
				<track_effect>false</track_effect>
				<draw_shadow>false</draw_shadow>
				<offset_by_height>false</offset_by_height>
				<spawn_offset_x>0</spawn_offset_x>
				<spawn_offset_y>0</spawn_offset_y>
				<scale_proportional>false</scale_proportional>
				<invert_scale_on_bounce>false</invert_scale_on_bounce>
				<add_emitter_direction>true</add_emitter_direction>
				<layer_offset>0</layer_offset>
				<depth_bias>-256</depth_bias>
				<enable_damage>false</enable_damage>
				<constant_damage>false</constant_damage>
				<damage_offset_x>0</damage_offset_x>
				<damage_offset_y>0</damage_offset_y>
				<damage_radius>0</damage_radius>
				<damage_type>None</damage_type>
			</settings>
			<curves>
				<Sprite_Frame start_min="0" start_max="0" end_min="0" end_max="4">
					<point x="0" y="1"/>
					<point x="1" y="1"/>
				</Sprite_Frame>
				<Direction start_min="0" start_max="360" end_min="0" end_max="360">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Direction>
				<Spawn_Rate start_min="100" start_max="100" end_min="1000" end_max="1000">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Spawn_Rate>
				<Velocity_X start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_X>
				<Velocity_Y start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Y>
				<Velocity_Height start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Height>
				<Velocity_Rotation start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Rotation>
				<Velocity_Direction start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Direction>
				<Color_R start_min="0.6" start_max="0" end_min="1" end_max="1">
					<point x="0" y="0"/>
					<point x="1" y="0"/>
				</Color_R>
				<Color_G start_min="0" start_max="0" end_min="0.8" end_max="1">
					<point x="0" y="1"/>
					<point x="1" y="1"/>
				</Color_G>
				<Color_B start_min="0.6" start_max="0" end_min="1" end_max="1">
					<point x="0" y="0"/>
					<point x="1" y="0"/>
				</Color_B>
				<Color_A start_min="0" start_max="0" end_min="1" end_max="1">
					<point x="0" y="1"/>
					<point x="0.915676" y="1"/>
					<point x="1" y="0"/>
				</Color_A>
				<Scale_X start_min="0.6" start_max="0.6" end_min="1" end_max="1">
					<point x="0" y="0"/>
					<point x="1" y="1"/>
				</Scale_X>
				<Scale_Y start_min="0.6" start_max="0.6" end_min="1" end_max="1">
					<point x="0" y="0"/>
					<point x="1" y="1"/>
				</Scale_Y>
				<Lifetime start_min="0" start_max="0" end_min="30" end_max="30">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Lifetime>
				<Rotation start_min="0" start_max="0" end_min="0" end_max="9">
					<point x="0" y="1"/>
					<point x="1" y="1"/>
				</Rotation>
				<Height start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Height>
				<Damage start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Damage>
			</curves>
			<events/>
		</emitter>
	</emitters>
	<attractors/>
</xml>

