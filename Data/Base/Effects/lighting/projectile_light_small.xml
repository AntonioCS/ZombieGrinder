<xml>
	<settings>
		<name>projectile_light_small</name>
	</settings>
	<emitters>
		<emitter> 
			<settings>
				<name>Projectile Light</name>
				<important>false</important>
				<render_pass>lighting</render_pass>
				<lock_rotation_to_direction>true</lock_rotation_to_direction>
				<keep_effect_alive>true</keep_effect_alive>
				<global_max_particles>0</global_max_particles>
				<draw_method>Sprite</draw_method>
				<blend_mode>Lighten</blend_mode>
				<cycle_length>1</cycle_length>
				<cycle_count>0</cycle_count>
				<max_particles_per_cycle>0</max_particles_per_cycle>
				<allow_bouncing>false</allow_bouncing>
				<beam_use_seperate_tail_head>false</beam_use_seperate_tail_head>
				<sprite>particle_large_point</sprite>
				<affected_by_attractors>false</affected_by_attractors>
				<move_based_on_direction>true</move_based_on_direction>
				<enable_collision>false</enable_collision>
				<collision_reponse>Stop</collision_reponse>
				<spawn_shape>Point</spawn_shape>
				<collision_scale_x>1</collision_scale_x>
				<collision_scale_y>1</collision_scale_y>
				<collision_offset_x>0</collision_offset_x>
				<collision_offset_y>0</collision_offset_y>
				<spawn_inner_radius>10</spawn_inner_radius>
				<spawn_outer_radius>10</spawn_outer_radius>
				<track_effect>true</track_effect>
				<draw_shadow>false</draw_shadow>
				<offset_by_height>false</offset_by_height>
				<spawn_offset_x>0</spawn_offset_x>
				<spawn_offset_y>0</spawn_offset_y>
				<scale_proportional>true</scale_proportional>
				<invert_scale_on_bounce>false</invert_scale_on_bounce>
				<add_emitter_direction>true</add_emitter_direction>
				<layer_offset>0</layer_offset>
				<depth_bias>0</depth_bias>
				<penetration_speed_scalar>1</penetration_speed_scalar>
				<enable_damage>false</enable_damage>
				<constant_damage>false</constant_damage>
				<damage_offset_x>0</damage_offset_x>
				<damage_offset_y>0</damage_offset_y>
				<damage_radius>0</damage_radius>
				<damage_type>Projectile</damage_type>
				<collection_enabled>false</collection_enabled>
				<collection_value>0</collection_value>
				<collection_type>Coin</collection_type>
			</settings>
			<curves>
				<Sprite_Frame start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Sprite_Frame>
				<Direction start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Direction>
				<Spawn_Rate start_min="18" start_max="18" end_min="18" end_max="18">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Spawn_Rate>
				<Velocity_X start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_X>
				<Velocity_Y start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Y>
				<Velocity_Height start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Height>
				<Velocity_Rotation start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Rotation>
				<Velocity_Direction start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Velocity_Direction>
				<Color_R start_min="0" start_max="0" end_min="0.7" end_max="0.7">
					<point x="0" y="1"/>
					<point x="1" y="1"/>
				</Color_R>
				<Color_G start_min="0" start_max="0" end_min="0.7" end_max="0.7">
					<point x="0" y="0.452212"/>
					<point x="1" y="0.430089"/>
				</Color_G>
				<Color_B start_min="0" start_max="0" end_min="0.7" end_max="0.7">
					<point x="0" y="0.2"/>
					<point x="1" y="0.2"/>
				</Color_B>
				<Color_A start_min="1.0" start_max="1.0" end_min="0.0" end_max="0.0">
					<point x="0" y="0"/>
					<point x="1" y="1"/>
				</Color_A>
				<Scale_X start_min="0.2" start_max="0.2" end_min="0.0" end_max="0.0">
					<point x="0" y="0"/>
					<point x="1" y="1"/>
				</Scale_X>
				<Scale_Y start_min="0.2" start_max="0.2" end_min="0.0" end_max="0.0">
					<point x="0" y="0"/>
					<point x="1" y="1"/>
				</Scale_Y>
				<Lifetime start_min="0.2" start_max="0.2" end_min="0.2" end_max="0.2">
					<point x="0" y="1"/>
					<point x="1" y="1"/>
				</Lifetime>
				<Rotation start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Rotation>
				<Height start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Height>
				<Damage start_min="0" start_max="0" end_min="0" end_max="0">
					<point x="0" y="1"/>
					<point x="1" y="0"/>
				</Damage>
			</curves>
			<events/>
		</emitter>
	</emitters>
	<attractors/>
</xml>

